<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE cases PUBLIC "-//AGF, Inc.//DTD stories 2.0//EN" "http://cases.dtd">
<cases>
    <include-entities file="entities.xml"/>
    <case id="NominalCase">
        <input>
            <create-entity name="Entity" id="myEntity">
                <parameter name="param1" value="value1"/>
                <parameter name="invalidParam" value="invalidParamValue"/>
            </create-entity>
        </input>
        <output>
            <copy-entity entity="myEntity" id="anotherEntity"/>
        </output>
    </case>
</cases>